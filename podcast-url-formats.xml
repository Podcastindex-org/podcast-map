<!-- Author: Christopher Isene<christopher.isene@gmail.com> -->
<rulesets>

  <ruleset name="Anchor.fm">
    <matching>^http(s)?\x3a\x2f\x2fanchor\x2efm\x2fs\x2f([0-9a-f]{1,})\x2fpodcast\x2frss$</matching>
  </ruleset>

